<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/splachScreenFragment">

    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.app.meditar.InicioFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_inicioFragment_to_oneFragment"
            app:destination="@id/oneFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_twoFragment"
            app:destination="@id/twoFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_threeFragment"
            app:destination="@id/threeFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_forFragment"
            app:destination="@id/forFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_fiveFragment"
            app:destination="@id/fiveFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_sixFragment"
            app:destination="@id/sixFragment" />
    </fragment>
    <fragment
        android:id="@+id/oneFragment"
        android:name="com.app.meditar.OneFragment"
        android:label="fragment_one"
        tools:layout="@layout/fragment_one" />
    <fragment
        android:id="@+id/twoFragment"
        android:name="com.app.meditar.TwoFragment"
        android:label="fragment_two"
        tools:layout="@layout/fragment_two" />
    <fragment
        android:id="@+id/threeFragment"
        android:name="com.app.meditar.ThreeFragment"
        android:label="fragment_three"
        tools:layout="@layout/fragment_three" />
    <fragment
        android:id="@+id/forFragment"
        android:name="com.app.meditar.ForFragment"
        android:label="fragment_for"
        tools:layout="@layout/fragment_for" />
    <fragment
        android:id="@+id/fiveFragment"
        android:name="com.app.meditar.FiveFragment"
        android:label="fragment_five"
        tools:layout="@layout/fragment_five" />
    <fragment
        android:id="@+id/sixFragment"
        android:name="com.app.meditar.SixFragment"
        android:label="fragment_six"
        tools:layout="@layout/fragment_six" />
    <fragment
        android:id="@+id/splachScreenFragment"
        android:name="com.app.meditar.SplachScreenFragment"
        android:label="fragment_splach_screen"
        tools:layout="@layout/fragment_splach_screen" >
        <action
            android:id="@+id/action_splachScreenFragment_to_inicioFragment"
            app:destination="@id/inicioFragment" />
    </fragment>
</navigation>